<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Report</title>
</head>
<body>

{% if data.links %}
    <h3>Links:</h3>
    <ol>
        {% for item in data.links %}
            <li>{{ item }}</li>
        {% endfor %}
    </ol>
{% endif %}

{% if data.buttons %}
    <h3>Buttons:</h3>
    <ol>
        {% for item in data.buttons %}
            <li>{{ item }}</li>
        {% endfor %}
    </ol>
{% endif %}

</body>
</html>
